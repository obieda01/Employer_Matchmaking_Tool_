@using Capstone.Web.Models;

@model List<Interview>

@{
    ViewBag.Title = "ViewMySchedule";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link type="text/css" rel="stylesheet" href="~/Content/css/login.css" />

<div class="wrapper">
    <h2>View My Schedule</h2>
    <table>
        <tr>
            <th>Employer Name</th>
            <th>Team Id</th>
            <th>Event Date</th>
            <th>Start Time</th>
        </tr>
        @foreach (Interview i in Model)
        {
            <tr>
                <td> @i.EmployerName</td>
                <td> @i.TeamId</td>
                <td> @i.EventDate</td>
                <td> @i.StartTime</td>
            </tr>
        }
    </table>

</div>