@using Capstone.Web.Models;

@model List<Interview>

@{
    ViewBag.Title = "ViewEmployerSchedule";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>View Employer Schedule</h2>

<table>
    <tr>
        <th>Student Name</th>
        <th>Team Id</th>
        <th>Event Date</th>
        <th>Start Time</th>
    </tr>
    @foreach (Interview i in Model)
    {
        <tr>
            <td> @i.StudentName</td>
            <td> @i.TeamId</td>
            <td> @i.EventDate</td>
            <td> @i.StartTime</td>
        </tr>
    }
</table>